<%= render @contact %>

<div class="container-fluid">
  <%= link_to "Edit this contact", edit_contact_path(@contact), class:"btn btn-secondary mt-5" %>
  <%= link_to "Back to contacts", contacts_path, class:"btn btn-secondary mt-5" %>
  <%= button_to "Destroy this contact", @contact, method: :delete, class:"btn btn-danger mt-5" %>
</div>
